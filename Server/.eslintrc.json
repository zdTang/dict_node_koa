{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
       "linebreak-style":0,
     "import/no-dynamic-require":0,
   "no-param-reassign":"off",
   "func-names":"off",
   "comma-dangle":"off",
   "spaced-comment":"off",
   "indent":"off",
   "no-console":"off",
   "prefer-destructuring":"off",
   "no-var":"off",
   "no-underscore-dangle":"off",
   "vars-on-top":"off",
   "quotes":"off",
   "no-bitwise":"off",
   "max-len":"off",
   "no-plusplus":"off",
   "no-continue":"off",
   "no-cond-assign":"off"
    }
}
